(+ 1 1)

(+ 1 (+ 1 1))

(= 3 (+ (+ 1 1) 1))

(not (> 1 2))

(list 1 2)

(car (cdr (list 1 2)))

(define (add a b) (+ a b))
(add 1 1)

(define (fact x) (if (= x 0) 1 (* x (fact (- x 1)))))
(fact 10)

(define (map f l) (if (not (null? l)) (cons (f (car l)) (map f (cdr l)))))
(map (lambda (x) (* x x)) (list 1 2 3))

